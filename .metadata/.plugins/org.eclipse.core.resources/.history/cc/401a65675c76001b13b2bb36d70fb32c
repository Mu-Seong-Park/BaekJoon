import java.io.*;
import java.util.*;

public class age_sort {

	public static void main(String[] args) throws IOException {
		
		BufferedReader bf = new BufferedReader(
				new InputStreamReader(System.in));
		
		int n;
		n = Integer.parseInt(bf.readLine());
		
		String[][] people = new String[n][3];
		String[] temp = new String[2];
		String change;
		
		for(int i = 0 ; i < n ; i++)
		{
			temp = bf.readLine().split(" ");
			people[i][0] = temp[0];
			people[i][1] = temp[1];
			people[i][2] = Integer.toString(i);
		}
		
		for(int j = 0 ; j < n ; j++)
		{
			for(int i = 0 ; i < n - 1 ; i++)
			{
				if(Integer.parseInt(people[i][0]) > Integer.parseInt(people[i+1][0]))
				{
					change = people[i][0];
					people[i][0] = people[i+1][0];
					people[i+1][0] = change;
					change = people[i][1];
					people[i][1] = people[i+1][1];
					people[i+1][1] = change;
					change = people[i][2];
					people[i][2] = people[i+1][2];
					people[i+1][2] = change;
				}
				else if(Integer.parseInt(people[i][0]) == Integer.parseInt(people[i+1][0]))
				{
					if(Integer.parseInt(people[i][2]) > Integer.parseInt(people[i+1][2]))
					{
						change = people[i][0];
						people[i][0] = people[i+1][0];
						people[i+1][0] = change;
						change = people[i][1];
						people[i][1] = people[i+1][1];
						people[i+1][1] = change;
						change = people[i][2];
						people[i][2] = people[i+1][2];
						people[i+1][2] = change;
					}
				}
			}
		}
		
		
		for(int i = 0 ; i < n ; i++)
		{
			System.out.println(people[i][0]+" "+people[i][1]);
		}
		bf.close();
	}

}
