package reference;

public class Student {
	int studentID;
	String studentName;
	
	Subject korea;
	Subject math;
	
	Student()
	{
		korea = new Subject();
		math = new Subject();
	}
	
	Student(int id, String name)
	{
		this.studentID = id;
		this.studentName = name;
		
		korea = new Subject();
		math = new Subject();
	}
	
	public void setKorea(String name, int score)
	{
		korea.setScore(score);
		korea.setSubjectName(name);
	}
	
}
