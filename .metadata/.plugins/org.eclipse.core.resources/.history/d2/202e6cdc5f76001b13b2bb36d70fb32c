import java.io.*;
import java.util.*;

public class age_sort {

	public static void main(String[] args) throws IOException {
		
		BufferedReader bf = new BufferedReader(
				new InputStreamReader(System.in));
		
		int n;
		n = Integer.parseInt(bf.readLine());
		
		String[][] people = new String[n][2];
		String[] temp = new String[n];
		String[] order = new String[n];
		String[] change = new String[2];
		String[] people_temp = new String[2];
	
		for(int i = 0 ; i < n ; i++)
		{
			people[i][0] = Integer.toString(i);
			people[i][1] = bf.readLine();
		}
		
		for(int i = 0 ; i < n ; i++)
		{
			temp[i] = people[i][1];
		}
		
		Arrays.sort(temp);	
		
		for(int i = 0 ; i < n ; i++)
		{
			change = temp[i].split(" ");
			people_temp = people[i][1].split(" ");
			if(change[0] == people_temp[0] && change[1] != people_temp[1])
			{
				for(int j = 0 ; j < n ; j++)
				{
					if((change[0]+" "+change[1]) == people[j][1] && Integer.parseInt(people[i][0]) > Integer.parseInt(people[j][0]) && i < j)
					{
						change[0] = people[i][0]; 
						change[1] = people[i][1]; 
						people[i][0] = people[j][0];
						people[i][1] = people[j][1];
						people[j][0] = change[0];
						people[i][1] = change[1];
					}
				}
			}
			
		}
		
		for(int i = 0 ; i < n ; i++)
		{
			System.out.println(temp[i]);
		}
		bf.close();
	}

}

