import java.util.*;
import java.io.*;

public class Num_Card2_10816 {
	
	public static int binarySearch(String[] pool, String question, int start, int end, int result)
	{
		if(start < 0 || start > end || end < 0)
		{
			return 0;
		}
		
		int mid = (start + end) / 2;
		if(pool[mid].equals(question))
		{
			
			result = result + binarySearch(pool, question,mid-1, end, result);
			return result + 1;
		}
		else if(Integer.parseInt(pool[mid]) > Integer.parseInt(question))
		{
			binarySearch(pool, question,start, mid - 1, result);
		}
		else 
		{
			binarySearch(pool, question,mid + 1, end, result);
		}
		
		return 0;
	}

	public static void main(String[] args) throws NumberFormatException, IOException {
		
		BufferedReader bf = new BufferedReader(
				new InputStreamReader(System.in));
		
		int n = Integer.parseInt(bf.readLine());
		
		String[] pool = new String[n];
		
		pool = bf.readLine().split(" ");
		
		int m = Integer.parseInt(bf.readLine());
		
		String[] question = new String[m];
		int temp = 0;
		
		question = bf.readLine().split(" ");
		
		Arrays.sort(pool);
		
		System.out.println(pool);
		for(int i = 0 ; i < m ; i++)
		{
			temp = binarySearch(pool,question[i],0,pool.length - 1,0);
			System.out.print(temp + " ");
		}
	
		
		
		bf.close();
		
	}

}
