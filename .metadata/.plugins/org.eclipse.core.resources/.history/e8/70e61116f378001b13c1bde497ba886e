
public class Student {
	int sno;
	String studentName;
	int grade;
	String address;
	
	Student(){
		
	}
	
	public void showStudentInfo() 
	{
		System.out.println(studentName + "," + address);
	}
	
	public static void main(String[] args) {
		
		Student s1 = new Student(); // 디폴트 생성자.
		s1.sno = 201611204;
		s1.studentName = "박무성";
		s1.address = "대구 북구 복현동 330-44번지";
		s1.grade = 4;
		s1.showStudentInfo();
	}
}
