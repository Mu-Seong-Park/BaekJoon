class Stack2{
	
	Object[] stack;
	int index = -1;
	int size;
	
	Stack2(int size)
	{
		this.size = size;
		this.stack = new Object[size];
	}
	
	public void push(int x)
	{
		
	}
	
	public int pop()
	{
		int temp;
		
		return temp;
	}
	
	public int top()
	{
		int temp;
		
		return temp;
	}
	
	public int getSize()
	{
		return index + 1;
	}
	
	public boolean isFull()
	{
		if(index == size - 1)
		{
			return true;
		}
		else
		{
			return false;
		}
	}
	
	public boolean isEmpty()
	{
		if(index == - 1)
		{
			return true;
		}
		else
		{
			return false;
		}
	}
	
